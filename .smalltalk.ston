SmalltalkCISpec {
  #preTesting : SCICustomScript {
    #path : 'ci/pretesting.st'
  },
  #loading : [
    SCIMetacelloLoadSpec {
      #baseline : 'FamixDart',
      #directory : 'src',
      #platforms : [ #pharo ],
      #onConflict : #useIncoming,
      #onUpgrade : #useIncoming
    }
  ],
  #testing : {
    #coverage : {
      #packages : [ 'Famix-Dart-Entities', 'Famix-Dart-Importer', 'Famix-Dart-Importer-Tests' ],
      #format : #lcov
    }
  }
}